<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
    </startup>
  <appSettings>
    <add key="UnSeenList" value="1"/> <!--Seconds-->
    <add key ="KeyFrame"  value="30"/><!--Seconds-->
    <add key="RemoveTimeout" value="60"/><!--Seconds-->
    <add key="RNCCheckInterval" value="60"/><!--Seconds-->
   <add key="defaultConnection" value="sqlCon" />
    <add key="rnc-ipaddress" value="192.168.1.55"/>
  </appSettings>

  <connectionStrings>
    <add name="sqlCon" connectionString="Data Source=.;Initial Catalog=DeviceManager;Trusted_Connection=true;" providerName="System.Data.SqlClient"/>
  </connectionStrings>
</configuration>